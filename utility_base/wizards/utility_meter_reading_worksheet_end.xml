<openerp>
<data>
<record id="utility_meter_reading_worksheet_end_view_form" model="ir.ui.view">
    <field name="name">utility.meter_reading_worksheet_end form</field>
    <field name="model">utility.meter_reading_worksheet_end</field>
    <field name="arch" type="xml">
        <form>
            <group name="header" colspan="4" col="2">
                <field name="date_end"/>
            </group>
            <footer>
                <button name="action_end" string="End" class="oe_highlight" type="object"/>
                or
                <button special="cancel" string="Cancel" class="oe_link"/>
            </footer>
        </form>
    </field>
</record>

<record id="utility_meter_reading_worksheet_end_action" model="ir.actions.act_window">
    <field name="name">End Worksheet</field>
    <field name="type">ir.actions.act_window</field>
    <field name="res_model">utility.meter_reading_worksheet_end</field>
    <field name="view_type">form</field>
    <field name="view_mode">form</field>
    <field name="target">new</field>
</record>

</data>
</openerp>

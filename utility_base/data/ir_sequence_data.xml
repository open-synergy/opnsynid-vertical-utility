</record>

<record id="sequence_meter_reading_worksheet" model="ir.sequence">
    <field name="name">Meter Reading Worksheet</field>
    <field name="code">mtr.read.sheet</field>
    <field name="padding">6</field>
    <field name="prefix">WRK/%(year)s/</field>
    <field eval="1" name="number_next"/>
    <field eval="1" name="number_increment"/>
</record>
</data>

    <field name="name">Meter Reading</field>
    <field name="code">meter.reading</field>
</record>

<record id="seq_type_meter_reading_worksheet" model="ir.sequence.type">
    <field name="name">Meter Reading Worksheet</field>
    <field name="code">mtr.read.sheet</field>
</record>

